{
  "name": "backend",
  "version": "2.0.0",
  "description": "Backend API with TypeORM and TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "seed:admin": "tsx src/seeds/create-admin.ts",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run -d src/config/database.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/database.ts",
    "schema:sync": "npm run typeorm -- schema:sync -d src/config/database.ts",
    "schema:drop": "npm run typeorm -- schema:drop -d src/config/database.ts"
  },
  "keywords": ["api", "typeorm", "express", "typescript"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "pg": "^8.18.0",
    "@supabase/supabase-js": "^2.28.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.24",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
